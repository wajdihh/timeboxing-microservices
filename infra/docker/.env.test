# Security Configuration
ENCRYPTION_KEY=test-encryption-key

# Third-party API Keys
SENDGRID_API_KEY=your_sendgrid_key  
STRIPE_API_KEY=your_stripe_key

# Rate Limiting
RATE_LIMIT_WINDOW=15
RATE_LIMIT_MAX=100

# CORS Configuration
ALLOWED_ORIGINS=http://localhost:3000
