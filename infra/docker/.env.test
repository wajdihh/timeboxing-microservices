# JWT
JWT_SECRET=test-access-secret
JWT_REFRESH_SECRET=test-refresh-secret
JWT_EXPIRATION=30s
JWT_REFRESH_EXPIRATION=2m

# Application Configuration
NODE_ENV=test
PORT=3000
GRAFANA_PORT=3001

# Database Configuration
DB_HOST=localhost
DB_PORT=5432
DB_USER=example_user
DB_PASSWORD=example_password
DB_NAME=identitydb
DATABASE_URL="postgresql://example_user:example_password@localhost:5432/identitydb?schema=e2e"

# REDIS
REDIS_HOST=localhost
REDIS_PORT=6379
